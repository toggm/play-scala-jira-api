# Routes
# This file defines all application routes (Higher priority routes first)
# ~~~~

# oauth services
GET		  /oauth								 controllers.OAuthController.index
POST      /oauth/requestToken                    controllers.OAuthController.obtainRequestToken(baseUrl:String, consumerKey:String,publicKey:String, callbackUrl:String) 
POST      /oauth/accessToken                     controllers.OAuthController.obtainAccessToken(baseUrl:String, consumerKey:String,publicKey:String, requestToken:String, tokenSecret:String, verifier:String)
